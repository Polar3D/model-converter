{
  "name": "@polar3d/model-converter",
  "version": "1.1.1",
  "description": "Universal 3D model format converter supporting GLB, GLTF, OBJ, STL, and 3MF formats",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "prepublishOnly": "npm run build",
    "dev": "rollup -c -w"
  },
  "keywords": [
    "3d",
    "converter",
    "stl",
    "gltf",
    "glb",
    "obj",
    "3mf",
    "three.js",
    "3d-printing",
    "model-conversion",
    "polar3d"
  ],
  "author": "Polar3D",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Polar3D/model-converter.git"
  },
  "bugs": {
    "url": "https://github.com/Polar3D/model-converter/issues"
  },
  "homepage": "https://github.com/Polar3D/model-converter#readme",
  "peerDependencies": {
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/three": "^0.160.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "three": "^0.160.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}
